{
  "name": "wordzuki",
  "version": "0.0.1",
  "description": "an app to save words",
  "main": "index.js",
  "scripts": {
    "start": "nodemon ./src/server/server.js",
    "watch": "webpack --progress --watch",
    "dev": "webpack-dev-server --hot --inline",
    "lint": "eslint src/server --color",
    "css": "npm run build:css && npm run build:minify && tachyons ./src/client/assets/styles/main.css > ./src/client/assets/styles/index.css --minify",
    "main:css": "tachyons ./src/client/assets/styles/main.css > ./src/client/assets/styles/index.css --minify",
    "build:css": "tachyons node_modules/tachyons/src/tachyons.css > node_modules/tachyons/css/tachyons.css",
    "build:minify": "tachyons node_modules/tachyons/src/tachyons.css -m > node_modules/tachyons/css/tachyons.min.css",
    "test": "NODE_ENV=test tape -r babel-register ./test/**/*-test.js | tap-spec"
  },
  "author": "Alex Wong",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^3.10.2",
    "eslint-config-airbnb": "^13.0.0",
