    "async": "^2.1.4",
    "axios": "^0.15.3",
    "babel": "^6.5.2",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-preset-stage-3": "^6.17.0",
    "babel-register": "^6.18.0",
    "babelify": "^7.3.0",
    "body-parser": "^1.15.2",
    "chai": "^3.5.0",
    "connect-redis": "^3.1.0",
    "cors": "^2.8.1",
    "css-loader": "^0.25.0",
    "debounce": "^1.0.0",
    "express": "^4.14.0",
    "express-session": "^1.14.2",
